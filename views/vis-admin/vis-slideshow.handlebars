<section id="dashboard">
    <div class="wrapper ">
        <div class="sidebar" data-color="purple" data-background-color="white" data-image="../assets/img/sidebar-1.jpg">
            {{>_navEditPages}}
            <div class="content p-1">
                <div class="list-group-item">
                    <div class="d-flex">
                        <div class="mr-auto p-2">
                            <h2 class="display-4 titulo">Visualizar Slideshow</h2>
                        </div>
                        <div class="p-2">
                            <span class="d-none d-md-block">
                                <a href="/admin/edit-home" class="btn btn-outline-primary btn-sm">Editar</a>
                            </span>
                            <div class="dropdown d-block d-md-none">
                                <button class="btn btn-primary dropdown-toggle btn-sm" type="button" id="acoesListar"
                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Ações
                                </button>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="acoesListar">
                                    <a class="dropdown-item" href="/admin/edit-home">Editar</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{>_msg}}
                    <hr>

                    <section id="home">
                        <!-- Start slides -->
                        <div id="slides" class="cover-slides">
                            <ul class="slides-container">
                                <li class="text-center">
                                    <!-- HEAD -->
                                    <img src="/images/slider-01.jpg" alt="">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h1 class="m-b-20"><strong>{{slideshow.dataValues.title1_1}} <br>
                                                        {{slideshow.dataValues.title1_2}}</strong></h1>
                                                <p class="m-b-40">{{slideshow.dataValues.text1_1}} <br>
                                                    {{slideshow.dataValues.text1_2}}</p>
                                                <p><a class="btn btn-lg btn-circle btn-outline-new-white"
                                                        href="{{slideshow.dataValues.urlbtn1}}">{{slideshow.dataValues.btn1}}</a>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li class="text-center">
                                    <!-- HEAD -->
                                    <img src="/images/slider-02.jpg" alt="">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h1 class="m-b-20"><strong>{{slideshow.dataValues.title2_1}}<br>
                                                        {{slideshow.dataValues.title2_2}} </strong></h1>
                                                <p class="m-b-40">{{slideshow.dataValues.text2_1}} <br>
                                                    {{slideshow.dataValues.text2_2}}</p>
                                                <p><a class="btn btn-lg btn-circle btn-outline-new-white"
                                                        href="{{slideshow.dataValues.urlbtn2}}">{{slideshow.dataValues.btn2}}</a>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li class="text-center">
                                    <!--- HEAD -->
                                    <img src="/images/slider-03.jpg" alt="">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h1 class="m-b-20"><strong>{{slideshow.dataValues.title3_1}}<br>
                                                        {{slideshow.dataValues.title3_2}}</strong></h1>
                                                <p class="m-b-40">{{slideshow.dataValues.text3_1}} <br>
                                                    {{slideshow.dataValues.text3_2}}</p>
                                                <p><a class="btn btn-lg btn-circle btn-outline-new-white"
                                                        href="{{slideshow.dataValues.urlbtn3}}">{{slideshow.dataValues.btn3}}</a>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                            <div class="slides-navigation">
                                <a href="#" class="next"><i class="{{slideshow.dataValues.iconbtnNext}}"></i></a>
                                <a href="#" class="prev"><i class="{{slideshow.dataValues.iconbtnPrev}}"
                                        aria-hidden="true"></i></a>
                            </div>
                        </div>
                </div>
            </div>
        </div>
    </div>
</section>