<div class="dashboard-wrapper">
    <div class="container-fluid  dashboard-content">
        <!-- ============================================================== -->
        <!-- pageheader -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="page-header">
                    <h2 class="pageheader-title">Formulário Adionar Aperitivos</h2>
                    <p class="pageheader-text">Proin placerat ante duiullam scelerisque a velit ac porta, fusce sit
                        amet vestibulum mi. Morbi lobortis pulvinar quam.</p>
                    <div class="page-breadcrumb">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a></li>
                                <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Forms</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Form Validations</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end pageheader -->
        <!-- ============================================================== -->
        {{>_msg}}

        <!-- ============================================================== -->
        <!-- horizontal form -->
        <!-- ============================================================== -->
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="card">
                <h5 class="card-header text-center">Visualizar a Página de Contato</h5>
                <div class="btn-group text-center col-12 mt-3" role="group" aria-label="Basic example">
                    <p class="col-4"></p>
                    <a href="/admin/edit-contato" class=""><button type="button" class="btn btn-success  ">Editar
                            Página Contato</button></a>
                    <a href="/visAdmin" class="ml-1"><button type="button" class="btn btn-primary  ">Menu Visualizar
                            Paginas</button></a>
                </div>
                <hr>
                <div class="card-body">
                    <div class="contact-box">
                        <div class="container">
                            <div class="all-page-title page-breadcrumb">
                                <div class="container text-center">
                                    <div class="row mt-5">
                                        <div class="col-lg-12">
                                            <h1>{{contato.dataValues.title}}</h1>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="heading-title text-center">
                                        <h2>{{contato.dataValues.subtitle}}</h2>
                                        <p>{{contato.dataValues.desc}}</p>
                                    </div>
                                </div>
                            </div>
                            <div class="row mt-3">
                                <div class="col-lg-12">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="name" name="name"
                                                    placeholder="{{contato.dataValues.placeholder1}}" required
                                                    data-error="Por favor:  {{contato.dataValues.placeholder1}}">
                                                <div class="help-block with-errors"></div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <input type="text" placeholder="{{contato.dataValues.placeholder2}}"
                                                    id="email" class="form-control" name="email" required
                                                    data-error="Por favor:  {{contato.dataValues.placeholder2}}">
                                                <div class="help-block with-errors"></div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <select class="custom-select d-block form-control" id="nota" name="nota"
                                                    required data-error="Please Select Person">
                                                    <option disabled selected>Dê uma nota</option>
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                    <option value="4">4</option>
                                                    <option value="5">5</option>
                                                </select>
                                                <div class="help-block with-errors"></div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <textarea class="form-control" id="message" name="message"
                                                    placeholder="{{contato.dataValues.placeholder4}}" rows="4"
                                                    data-error="Por favor: {{contato.datavalues.placeholder4}}"
                                                    required></textarea>
                                                <div class="help-block with-errors"></div>
                                            </div>
                                            <div class="submit-button text-center">
                                                <button class="btn btn-common" id="submit"
                                                    type="submit">{{contato.dataValues.title_btn}}</button>
                                                <div id="msgSubmit" class="h3 text-center hidden"></div>
                                                <div class="clearfix"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end horizontal form -->
